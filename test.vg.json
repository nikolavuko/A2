{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "values": [
        {"month": 1, "continent": "Asia", "new_cases": 1000},
        {"month": 1, "continent": "Europe", "new_cases": 2000},
        {"month": 2, "continent": "Asia", "new_cases": 1500},
        {"month": 2, "continent": "Europe", "new_cases": 3000}
      ]
    },
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "month",
        "type": "ordinal",
        "title": "Month of the year"
      },
      "y": {
        "field": "new_cases",
        "aggregate": "sum",
        "type": "quantitative",
        "title": "Total New Cases"
      },
      "color": {
        "field": "continent",
        "type": "nominal",
        "title": "Continent"
      }
    },
    "title": "Total New COVID-19 Cases by Month and Continent"
  }
  